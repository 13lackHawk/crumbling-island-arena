<root>
	<styles>
		<include src="file://{resources}/styles/custom_game/general_style.css" />
	</styles>
	<scripts>
		<include src="file://{resources}/scripts/custom_game/util.js" />
		<include src="file://{resources}/scripts/custom_game/simple_panel_animation.js" />
		<include src="file://{resources}/scripts/custom_game/general_script.js" />
	</scripts>
	<Panel class="MainPanel" hittest="false">
		<Image src="file://{images}/custom_game/zeus_q.png" style="opacity:0.0;"/>
		<Image src="file://{images}/custom_game/zeus_w.png" style="opacity:0.0;" />
		<Image src="file://{images}/custom_game/zeus_e.png" style="opacity:0.0;"/>
		<Image src="file://{images}/custom_game/zeus_r.png" style="opacity:0.0;"/>
		<Panel id="MainPanel" class="MainPanel" hittest="false">
			<Panel id="DebugPanel" hittest="false">
			</Panel>
			<Panel id="HeroPanel" hittest="false">

				<Panel class="HealthPanel" id="HealthPanel">
				</Panel>

				<Panel class="AbilityPanel" hittest="false" id="AbilityPanel">
				</Panel>
			</Panel>
			<Panel id="TimersPanel" hittest="false">
				<Label class="TimerDescription" text="Island collapses!"/>
				<Label id="Stage2Timer" class="TimerText" text=""/>

				<Label class="TimerDescription" text="Ultimates unlocked"/>
				<Label id="UltsTimer" class="TimerText" text=""/>

				<Label class="TimerDescription" text="Island collapses!!!"/>
				<Label id="Stage3Timer" class="TimerText" text=""/>

				<Label class="TimerDescription" text="You are doomed"/>
				<Label id="SuddenDeathTimer" class="TimerText" text=""/>
			</Panel>
		</Panel>
	</Panel>
</root>
